  {
    // layout name
    name[Group1]= "Russian (phonetic Mac)"

    // NUMBERS row
    ; key <TLDE>
       {[ Cyrillic_shcha
        , Cyrillic_SHCHA
        ]}
    ; key <AE01>
       {[ 1
        , exclam
        ]}
