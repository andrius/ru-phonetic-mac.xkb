       {[ 5
        , percent
        ]}
    ; key <AE06>
       {[ 6
        , asciicircum
        ]}
    ; key <AE07>
       {[ 7
        , ampersand
        ]}
    ; key <AE08>
       {[ 8
        , asterisk
        ]}
    ; key <AE09>
       {[ 9
        , parenleft
        ]}
    ; key <AE10>
       {[ 0
        , parenright
        ]}
    ; key <AE11>
       {[ Cyrillic_softsign
        , Cyrillic_SOFTSIGN
